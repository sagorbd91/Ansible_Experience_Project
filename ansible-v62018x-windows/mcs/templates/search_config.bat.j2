echo "modify fcs store"
set server={{ mcs_server_dir }}
set os=win_b64
set user=creator
set password={{ creator_password }}

if "%password%"=="" (
  set context=set context user %user%
) else (
  set context=set context user %user% password %password%
)

 call %server%\%os%\code\bin\mql -t -c "%context% ; set system searchindex file {{ config_xml_path }};
 echo "Set searchindex config is done"