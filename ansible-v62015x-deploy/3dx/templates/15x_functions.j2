

LOG_NAME=3dx
LOG_DATE=`date +"%y%m%d_%H%M%S"`
LOG_FILE_NAME=${LOG_NAME}.${LOG_DATE}.log
export LOG_FILE={{e3dx_logdir}}/${LOG_FILE_NAME}


CheckRunUser ()
{
  if [ -z "${USER}" ]
  then
    USER=`whoami`
  fi
  if [ "${USER}" != "$1" ]
  then
    echo " This script / function must be run with ID : ${1} !"
    echo " Exiting program !"
    exit 1
  fi
}

